{
    "port": 9000,
    "handlers": {
        "^/": "$./handlers/mvc"
    },
    "mvc": {
        "routes": {
            "/home": "./home",
            "/": "./home",
            "get /date": {
                "view": "./date"
            }
        }
    },
    "filters": {
        "^/": "../"
    },
    "proxy": {
        "rules": {
            "^/api": "http://localhost:8000"
        }
    }
}